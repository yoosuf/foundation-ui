{
  "name": "@yoosuf/foundation-ui",
  "version": "0.1.25",
  "description": "A modern, accessible Vue 3 component library",
  "type": "module",
  "main": "./dist/foundation-ui.umd.cjs",
  "module": "./dist/foundation-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/foundation-ui.js",
      "require": "./dist/foundation-ui.umd.cjs"
    },
    "./style.css": "./dist/foundation-ui.css"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && vue-tsc --declaration --emitDeclarationOnly && node --experimental-specifier-resolution=node prepare-dist.js",
    "preview": "vite preview",
    "test": "jest",
    "lint": "eslint . --ext .vue,.js,.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "component-library",
    "ui-kit",
    "typescript"
  ],
  "author": "Yoosuf Organization",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.12.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^2.2.0"
  }
}
